{
  "version": 4,
  "configurations": [
    {
      "name": "openFrameworks",
      "compilerPath": "clang++",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "clang-x64",
      "includePath": [
        "${workspaceFolder}/projects/*/src",
        "${workspaceFolder}/libs/openFrameworks/libs/**",
        "${workspaceFolder}/libs/openFrameworks/addons/**",
        "${workspaceFolder}/libs/openFrameworks/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/libs/openFrameworks/libs",
          "${workspaceFolder}/libs/openFrameworks/addons"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}